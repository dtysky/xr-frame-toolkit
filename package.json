{
  "name": "xr-frame-toolkit",
  "version": "0.8.0",
  "main": "dist/index.js",
  "scripts": {
    "prepare": "rm -rf dist && mkdir -p dist/common && cp src/service/common/tinyexr.wasm dist/common",
    "dev-client": "webpack serve",
    "dev-service": "npm run prepare && tsc -w -p ./tsconfig.service.json",
    "dev": "concurrently --kill-others \"npm run dev-client\" \"npm run dev-service\"",
    "build-client": "webpack --env isProd=true && cp -rf src/static dist",
    "build-service": "npm run prepare && tsc -p ./tsconfig.service.json",
    "build": "npm run build-service && npm run build-client",
    "start": "export NODE_ENV=development && electron ./dist",
    "package": "npm run build && rm -rf out && electron-forge package",
    "make": "npm run build && rm -rf out && electron-forge make",
    "test": "rm -rf out && electron-forge make"
  },
  "dependencies": {
    "gl-matrix": "^3.4.3",
    "gltf-pipeline": "^4.1.0",
    "hana-ui": "^1.3.4",
    "mime": "^3.0.0",
    "mmme-encoding": "^0.5.1",
    "ncp": "^2.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.11.2",
    "react-transition-group": "^4.4.5",
    "seinjs-texture-compressor": "^1.0.3",
    "sharp": "^0.32.1",
    "xyz-utils": "^0.5.2"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.2.1",
    "@electron-forge/maker-deb": "^6.2.1",
    "@electron-forge/maker-dmg": "^6.2.1",
    "@electron-forge/maker-squirrel": "^6.2.1",
    "@electron-forge/maker-wix": "^6.1.1",
    "@types/node": "^20.2.3",
    "@types/react": "^18.2.7",
    "concurrently": "^8.0.1",
    "css-loader": "^6.7.3",
    "electron": "^24.3.1",
    "electron-squirrel-startup": "^1.0.0",
    "html-webpack-plugin": "^4.5.0",
    "mini-css-extract-plugin": "^2.6.1",
    "postcss": "^8.4.23",
    "postcss-loader": "^7.0.1",
    "postcss-preset-env": "^7.8.3",
    "raw-loader": "^4.0.2",
    "sass": "^1.56.1",
    "sass-loader": "^13.1.0",
    "sass-resources-loader": "^2.2.5",
    "ts-loader": "^9.2.2",
    "typescript": "^4.7.2",
    "typescript-plugin-css-modules": "^3.4.0",
    "url-loader": "^4.1.0",
    "webpack": "^5.11.0",
    "webpack-cli": "^4.3.0",
    "webpack-dev-server": "^4.11.0"
  }
}
